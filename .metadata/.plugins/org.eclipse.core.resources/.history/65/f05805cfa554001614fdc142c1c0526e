package com.gky.thread;

public class Producter extends Thread{

	private ShareData data;
	
	public Producter(ShareData data) {
		this.data = data;
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		for(int i = 0; i < 26; i++){
			char ch = (char)(65+i);
			data.setData(ch);
			System.out.println("Product:"+ch);
		}
	}

}
